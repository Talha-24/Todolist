import React, { useState } from "react";



const App =()=>{
    const [userfirstname, setuserfirstname]=useState();
const [userfamilyname, setuserfamilyname]=useState();
const [useremail, setuseremail]=useState();
const [userphone, setuserphone]=useState();
const [userpassword,setuserpassword]=useState();
const [userconfirmpassword, setuserconfirmpassword]=useState();
const [datetimelocal, setdatetimelocal]=useState();

const formHandler=(e)=>{
    e.preventDefault();
    console.log("User : ",userfirstname," ",userfamilyname);
    console.log(userfirstname);
    setuserfirstname('');
    console.log(userfamilyname);
    setuserfamilyname('');
    
   console.log(userfirstname," ",userfamilyname);
    console.log(useremail);
    setuseremail('');
    console.log(userphone);
    setuserphone('');
    console.log(userpassword);
    setuserpassword('');
    console.log(userconfirmpassword);
    setuserconfirmpassword('');
    console.log(datetimelocal);
    setdatetimelocal('');

}
    return(
        <div>
            <form onSubmit={(e)=>{formHandler(e);}}>
            <div><p>First Name</p><input onChange={(e)=>{
                setuserfirstname(e.target.value);



            }}  type="text" placeholder="Enter your First name" value={userfirstname} /></div>
            <div><p>Last Name</p><input onChange={
                (e)=>{
              setuserfamilyname(e.target.value);

                }
            } type="text" placeholder="Enter your Last name" value={userfamilyname} /></div>
            <div><p>Email ID</p><input onChange={(e)=>{
                setuseremail(e.target.value);
            }} type="email" placeholder="Enter your email ID" value={useremail} /></div>
            <div><p>Phone Number</p><input type="phone" onChange={(e)=>{
                setuserphone(e.target.value);

            }} placeholder="Enter your Phone Number" value={userphone} /></div>
            <div><p>Password</p><input onChange={(e)=>{
                setuserpassword(e.target.value);
            }} type="password" placeholder="Enter your Password" value={userpassword} /></div>
            <div><p>Confirm Password</p><input onChange={(e)=>{
                setuserconfirmpassword(e.target.value);
            }} type="password" placeholder="Re-type your Password" value={userconfirmpassword}/></div>
            <div><p></p><input onChange={(a)=>{
                setdatetimelocal(a.target.value);
            }}type="datetime-local" value={datetimelocal} /></div>
            <button>Submit</button>
            </form>
        </div>
       
    )
}
export default App



