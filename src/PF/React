// export default App
import React from "react"

const App =()=>{
    const formHandler=(e)=>{
        e.preventDefault();
        console.log("Usernumber : 1");
        console.log("Username: ",username);
        setusername('');
        console.log("Useremail: ",useremail);
        setuseremail('');
        console.log("Phone Number: ",userphone);
        setuserphone('');
        console.log("Whattsapp Number: ",userwhattsapp);
        setuserwhattsapp('');
        console.log("IDCARD/BFORM: ",userbform);
        setuserbform('');
        console.log("Password: ",userpassword);
        setuserpassword('');
        console.log("Confirm Password: ",userconfirmpassword);
        setuserconfirmpassword('');
      

    }
    //React Variables to Manipulate DOM Indirectly(Virtualy DOM and then Real DOM);
    const [username,setusername]=useState();
    const [useremail,setuseremail]=useState();
    const [userphone,setuserphone]=useState();
    const [userwhattsapp,setuserwhattsapp]=useState();
    const [userbform,setuserbform]=useState();
    const [userpassword,setuserpassword]=useState();
    const [userconfirmpassword,setuserconfirmpassword]=useState();
    return(
        
        <div>
               
               <form onSubmit={(e)=>{
                formHandler(e);

            }}>
                <h1 className="text-3xl">Registeration Form</h1>
            <div><p>Name</p><input onChange={(a)=>{
            setusername(a.target.value)

        }
            } type="text" placeholder="Enter your name" value={username} /></div>
            <div><p>Email</p><input onChange={(a)=>

                {
                     setuseremail(a.target.value);
                }
            } type="email" placeholder="Enter your email" value={useremail} /></div>
            <div><p>Mobile Number</p><input onChange={(a)=>{
                setuserphone(a.target.value);
            }} type="text" placeholder="Enter your email" value={userphone} /></div>
            <div><p>Whattsapp Number</p><input onChange={(a)=>{
                setuserwhattsapp(a.target.value);
            }} type="text" placeholder="Enter your Whattsapp Number" value={userwhattsapp} /></div>
            <div><p>ID Number/B-Form</p><input
            onChange={(a)=>{
                setuserbform(a.target.value);
            }} type="text" placeholder="Enter your IDCARD / B-Form Number" value={userbform} /></div>
            <div><p>Security Password</p><input onChange={(a)=>{
                setuserpassword(a.target.value);
            }} type="password" placeholder="include letter, @, 123" value={userpassword} /></div>
            <div><p>Confirm Security Password</p><input onChange={(a)=>{
                setuserconfirmpassword(a.target.value);
            }} type="password" placeholder="Re-type Security Password" value={userconfirmpassword}/></div>
            <div><button>Submit</button></div>
            </form>
          
          
        </div>
    )
}
export default App